#include<stdio.h>
#include<condo.h>
Void main ()
{
  Int n,r,t,sum=0;
  Printf("enter a number:")
  scanf("%d",&n);
   t=n;
   While(n!=0);
  {
    r=n%10;
     sum=sum=(r*r*r);
     n=n/10;
   }
    if(t==sum)
    Printf("/n%d is Armstrong ")
     else
    printf("/n%d) is Armstrong
      else
    printf("/n%d is not an number",sum);
     getch();
}
